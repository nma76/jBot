{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Daemon": {
    "BaseHashTag": "#jonikabot",
    "Interval": 30,
    "Authentication": {
      "ConsumerKey": "<Add your key here>",
      "ConsumerKeySecret": "<Add your key secret here>",
      "AccessToken": "<Add your token here>",
      "AccessTokenSecret": "<Add your token secret here>"
    },
    "DataStore": {
      "DataFolder": "/tmp/",
      "DataFilePrefix": "jBot",
      "OverlayFolder": "/tmp/"
    }
  }
}